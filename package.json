{
  "name": "identity-reporting",
  "version": "0.0.0",
  "engines": {
    "npm": ">=7"
  },
  "scripts": {
    "build": "vite build",
    "postbuild": "npm run generate-routes && cp robots.txt _site/",
    "dev": "vite",
    "generate-routes": "node -r esbuild-register bin/generate-routes.js",
    "pages": "npm run build -- --base=${BASEURL:-/}",
    "prettier:fix": "npm run test:lint -- --fix",
    "serve": "vite preview",
    "test:lint": "eslint . --ext .js,.ts,.tsx",
    "test:typecheck": "tsc",
    "test:unit": "mocha -r esbuild-register -r global-jsdom/register -r ./mocha-setup.ts 'src/**/*.test.{js,ts,tsx}'",
    "test": "run-p test:*"
  },
  "dependencies": {
    "@observablehq/plot": "^0.5.0",
    "autoprefixer": "^10.3.4",
    "d3-array": "^3.0.2",
    "d3-axis": "^3.0.0",
    "d3-dsv": "^3.0.1",
    "d3-format": "^3.0.1",
    "d3-scale": "^4.0.1",
    "d3-scale-chromatic": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.0.1",
    "d3-time": "^3.0.0",
    "d3-time-format": "^4.0.0",
    "identity-style-guide": "^6.3.0",
    "preact": "^10.5.14",
    "preact-fetching": "^0.1.0",
    "preact-markdown": "^2.0.0",
    "preact-router": "^3.2.1"
  },
  "devDependencies": {
    "@18f/eslint-plugin-identity": "^2.0.0",
    "@testing-library/preact": "^2.0.1",
    "@testing-library/preact-hooks": "^1.1.0",
    "@types/chai": "^4.2.21",
    "@types/d3-array": "^3.0.1",
    "@types/d3-axis": "^3.0.1",
    "@types/d3-dsv": "^3.0.0",
    "@types/d3-format": "^3.0.1",
    "@types/d3-scale": "^4.0.1",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/d3-selection": "^3.0.1",
    "@types/d3-shape": "^3.0.2",
    "@types/d3-time": "^3.0.0",
    "@types/d3-time-format": "^4.0.0",
    "@types/jsdom": "^16.2.13",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "chai": "^4.3.4",
    "esbuild-register": "^3.0.0",
    "eslint": "^8.17.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-mocha": "^10.0.5",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fetch-mock": "^9.11.0",
    "global-jsdom": "^8.2.0",
    "jsdom": "^17.0.0",
    "mocha": "^9.2.2",
    "node-fetch": "^3.2.10",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.2",
    "sass": "^1.39.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.13"
  }
}
